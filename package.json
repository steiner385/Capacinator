{
  "name": "capacinator",
  "version": "1.0.0",
  "description": "Web-based project capacity planning system",
  "main": "src/electron/main-with-setup.cjs",
  "type": "module",
  "scripts": {
    "dev": "./scripts/start-dev.sh",
    "dev:stop": "./scripts/stop-dev.sh",
    "dev:logs": "./scripts/dev-logs.sh",
    "dev:cleanup": "./scripts/cleanup-orphaned-processes.sh",
    "dev:no-bg": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cross-env NODE_ENV=development tsx src/server/index.ts",
    "dev:client": "cross-env NODE_ENV=development vite --config client-vite.config.ts",
    "dev:electron": "wait-on http://localhost:3110 && electron .",
    "prod:start": "./scripts/start-prod.sh",
    "prod:stop": "./scripts/stop-prod.sh",
    "prod:logs": "./scripts/prod-logs.sh",
    "prod:frontend:start": "./scripts/start-prod-frontend.sh",
    "prod:frontend:stop": "./scripts/stop-prod-frontend.sh",
    "prod:frontend:logs": "./scripts/prod-frontend-logs.sh",
    "server:start": "./scripts/start-server.sh",
    "server:stop": "./scripts/stop-server.sh",
    "server:logs": "./scripts/server-logs.sh",
    "e2e:start": "./scripts/e2e-server-manager.sh start",
    "e2e:stop": "./scripts/e2e-server-manager.sh stop",
    "e2e:restart": "./scripts/e2e-server-manager.sh restart",
    "e2e:status": "./scripts/e2e-server-manager.sh status",
    "e2e:logs": "./scripts/e2e-server-manager.sh logs",
    "e2e:start:legacy": "./scripts/start-e2e-server.sh",
    "e2e:stop:cleanup": "./scripts/stop-e2e-server.sh --cleanup-db",
    "e2e:reset": "./scripts/stop-e2e-server.sh --cleanup-db && ./scripts/start-e2e-server.sh",
    "build": "npm run build:server && npm run build:client && npm run build:electron",
    "build:server": "tsc -p tsconfig.production.json",
    "build:client": "vite build --config client-vite.config.ts",
    "build:electron": "electron-builder",
    "test": "jest --config jest.config.cjs",
    "test:client": "jest --config jest.config.client.cjs",
    "test:client:watch": "jest --config jest.config.client.cjs --watch",
    "test:server": "jest --config jest.config.cjs",
    "test:unit": "npm run test:client && npm run test:server",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:smoke": "playwright test --project=smoke",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:crud": "playwright test --grep @crud",
    "test:e2e:reports": "playwright test tests/e2e/suites/reports",
    "test:e2e:codegen": "playwright codegen http://localhost:3120",
    "test:e2e:isolated": "playwright test --config playwright.e2e.config.ts",
    "test:e2e:isolated:ui": "playwright test --ui --config playwright.e2e.config.ts",
    "test:e2e:isolated:headed": "playwright test --headed --config playwright.e2e.config.ts",
    "test:e2e:isolated:debug": "playwright test --debug --config playwright.e2e.config.ts",
    "test:e2e:comprehensive": "tsx tests/e2e/run-all-scenarios.ts",
    "test:e2e:enterprise": "playwright test tests/e2e/enterprise-expansion.spec.ts",
    "test:e2e:agile": "playwright test tests/e2e/agile-product-development.spec.ts",
    "test:e2e:consulting": "playwright test tests/e2e/consulting-services.spec.ts",
    "test:scenarios": "jest --config=jest.scenario.config.js",
    "test:scenarios:unit": "jest --config=jest.scenario.config.js --testPathPattern=__tests__",
    "test:scenarios:e2e": "playwright test --config=playwright.scenario.config.ts",
    "test:scenarios:corruption": "playwright test --config=playwright.scenario.config.ts --project=scenario-corruption-tests",
    "test:scenarios:all": "./scripts/test-scenario-system.sh",
    "lint": "eslint src client/src shared --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "typecheck:shared": "cd shared && tsc --noEmit",
    "next-issue": "npx tsx scripts/get-next-issue.ts",
    "db:init": "tsx src/server/database/init.ts",
    "db:migrate": "tsx src/server/database/migrate.ts",
    "db:migrate-data": "tsx src/server/database/migrate-data.ts",
    "db:seed": "tsx src/server/database/seed.ts",
    "import:excel": "tsx scripts/import-excel.js",
    "import:excel:help": "tsx scripts/import-excel.js --help",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:linux": "npm run build && electron-builder --linux",
    "start": "electron .",
    "commands": "./scripts/list-commands.sh"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-table": "^8.21.3",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^6.4.17",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "diff": "^5.2.2",
    "dotenv": "^17.1.0",
    "electron-store": "^10.1.0",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "glob": "^11.0.3",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lucide-react": "^0.525.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "node-cron": "^4.2.0",
    "nodemailer": "^7.0.5",
    "puppeteer-core": "^24.12.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "recharts": "^3.0.2",
    "simple-git": "^3.30.0",
    "socket.io": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "build": {
    "appId": "com.capacinator.app",
    "productName": "Capacinator",
    "copyright": "Copyright Â© 2024 Capacinator",
    "directories": {
      "output": "dist-electron",
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "src/electron/*.cjs",
      "src/electron/*.html",
      "src/electron/*.js",
      "package.json",
      "node_modules/**/*",
      "!**/*.ts",
      "!**/tsconfig.json",
      "!**/*.map",
      "!**/*.spec.js",
      "!**/*.test.js",
      "!node_modules/**/test/**",
      "!node_modules/**/*.md"
    ],
    "asar": true,
    "compression": "store",
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "include": "build/installer.nsh",
      "warningsAsErrors": false,
      "perMachine": false
    },
    "extraMetadata": {
      "main": "src/electron/main-with-setup.cjs"
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@eslint/js": "^9.30.1",
    "@faker-js/faker": "^9.9.0",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/form-data": "^2.5.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.11",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/supertest": "^6.0.3",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^37.2.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "form-data": "^4.0.3",
    "github-issue-prioritizer": "file:../PriorityCalculator",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.24.0",
    "supertest": "^7.1.3",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^5.4.19",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.3"
  }
}
