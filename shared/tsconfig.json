{
  "compilerOptions": {
    "target": "ES2022",
    "module": "Node16",
    "moduleResolution": "Node16",
    "lib": ["ES2022"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "noEmit": true,
    "rootDir": ".",
    "baseUrl": ".",
    "types": [],
    "typeRoots": []
  },
  "include": ["types/**/*"],
  "exclude": ["node_modules"]
}
